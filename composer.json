{
    "name": "spinen/connectwise-php-client",
    "description": "SPINEN's PHP Client for ConnectWise.",
    "keywords": [
        "connectwise",
        "client",
        "spinen"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "~5.3|~6.0",
        "nesbot/carbon": "~1.20|^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.5",
        "psy/psysh": "0.8.*",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\ConnectWise\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
