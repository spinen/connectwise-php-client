{
    "name": "spinen/connectwise-php-client",
    "description": "SPINEN's PHP Client for ConnectWise.",
    "keywords": [
        "connectwise",
        "client",
        "spinen"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "illuminate/support": "~5.5|~6|~7|~8|~9",
        "nesbot/carbon": "~1.2|^2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "psy/psysh": "^0.10",
        "symfony/thanks": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\ConnectWise\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
