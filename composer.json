{
    "name": "spinen/connectwise-php-client",
    "description": "SPINEN's PHP Client for ConnectWise.",
    "keywords": [
        "connectwise",
        "client",
        "spinen"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^9.19|^10",
        "nesbot/carbon": "^2.62.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.5.8",
        "psy/psysh": "^0.11",
        "scrutinizer/ocular": "^1.9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\ConnectWise\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\ConnectWise\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
